{
  "name": "json-diff-ts-cjs",
  "version": "1.0.0",
  "description": "A JSON diff tool for JavaScript written in TypeScript. forked from: https://github.com/ltwlf/json-diff-ts.git",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest --config jest.config.mjs",
    "test:watch": "jest --watch --config jest.config.mjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagomayrink/json-diff-ts-cjs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "contributors": ["Christian Glessner","leitwolf_io"],
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.7",
    "eslint": "8.53.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "29.0.5",
    "typescript": "5.3.2"
  }
}